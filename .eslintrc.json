{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-invalid-this" : "off",
        "require-jsdoc" : "off",
        "linebreak-style" : "off",
        "indent": ["error", 2],
        "max-len": ["error", { "code": 100 }],
        "camelcase": "off"
    }
}
